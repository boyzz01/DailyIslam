<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4a5a6b"
    tools:context=".kiblat.Kiblat">

    <ImageView
        android:id="@+id/ic_compass"
        android:layout_width="298dp"
        android:layout_height="296dp"
        android:layout_gravity="center"
        android:background="@drawable/compass"
        app:srcCompat="@drawable/kompas" />

    <ImageView
        android:id="@+id/jarum_kiblat"
        android:layout_width="37dp"
        android:layout_height="183dp"
        android:layout_gravity="center"
        app:srcCompat="@drawable/jarum" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:adjustViewBounds="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mosque" />


</FrameLayout>